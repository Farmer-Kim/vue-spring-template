<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kotra.mgt.gm.common.code.CmmnCodeMapper">

	<!-- 공통코드 조회 -->
	<select id="cmmnCodeList" parameterType="xxx.mgt.gm.common.code.CmmnCodeVO" resultType="xxx.mgt.gm.common.code.CmmnCodeVO">
		SELECT CODE_ID
			 , CODE_NM
		  FROM PJH_COMM_CODE
		 WHERE GROUP_ID = #{searchGroupId}
		   AND USE_YN = 'Y'
		 ORDER BY SORT_SN
	</select>
	
	
</mapper>